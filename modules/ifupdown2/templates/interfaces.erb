# /etc/network/interfaces template for switches (<%=@hostname%>)
# Created by Puppet

auto lo
iface lo inet loopback
  address <%=@intvars['loopback']%>/32

auto eth0
iface eth0 inet dhcp

<% @intvars['neighbors'].each do |name, iface| -%>
auto <%=name%>
iface <%=name%>
    address <%=@intvars['loopback']%>/32

<% end %>

